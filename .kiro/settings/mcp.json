        "getRepositoryTag",
        "listAllNamespacesMemberOf",
        "listNamespaces",
        "listRepositoriesByNamespace",
        "listRepositoryTags",
        "search",
        "updateRepositoryInfo"
      ]
    },
    "context7": {
      "command": "npx",
