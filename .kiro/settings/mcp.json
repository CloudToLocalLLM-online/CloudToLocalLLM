        "DISABLE_CONSOLE_OUTPUT": "true",
        "N8N_API_URL": "https://api.n8n-mcp.com/mcp",
        "N8N_API_KEY": "nmcp_8c599709d6c0b1c3cf826da8b2b966fa2e81d6bc3c18a34582c9e735fdc67a64"
      }
