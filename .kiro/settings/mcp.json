{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "mcp_playwright_playwright_screenshot",
        "mcp_playwright_playwright_navigate",
        "mcp_playwright_playwright_click",
        "mcp_playwright_playwright_fill",
        "browser_navigate",
        "browser_take_screenshot"
      ]
    },
    "auth0": {
      "command": "npx",
      "args": [
        "@auth0/auth0-mcp-server",
        "run"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AUTH0_DOMAIN": "dev-v2f2p008x3dr74ww.us.auth0.com",
        "AUTH0_CLIENT_ID": "FuXPnevXpp311CdYHGsbNZe9t3D8Ts7A",
        "AUTH0_CLIENT_SECRET": ""
      },
      "disabled": false,
      "autoApprove": [
        "mcp_auth0_auth0_list_resource_servers",
        "auth0_list_resource_servers",
        "auth0_list_applications",
        "auth0_get_resource_server"
      ]
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "mcp_git_git_status",
        "mcp_git_git_log",
        "mcp_git_git_diff_unstaged",
        "git_status"
      ]
    },
    "sqlite": {
      "command": "uvx",
      "args": [
        "mcp-server-sqlite",
        "--db-path",
        "./data/app.db"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "mcp_sqlite_read_query",
        "mcp_sqlite_list_tables",
        "mcp_sqlite_describe_table",
        "list_tables"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "add_memory",
        "search_memory",
        "get_memory",
        "list_memories",
        "create_entities",
        "search_nodes"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "@upstash/context7-mcp"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "search_docs",
        "get_documentation",
        "list_libraries",
        "resolve-library-id"
      ]
    }
  }
}