{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "mcp_playwright_playwright_screenshot",
        "mcp_playwright_playwright_navigate",
        "mcp_playwright_playwright_click",
        "mcp_playwright_playwright_fill",
        "browser_navigate",
        "browser_take_screenshot"
      ]
    },
    "auth0": {
      "command": "npx",
      "args": [
        "@auth0/auth0-mcp-server",
        "run"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "mcp_auth0_auth0_list_resource_servers",
        "auth0_list_resource_servers",
        "auth0_list_applications",
        "auth0_get_resource_server",
        "auth0_list_clients",
        "auth0_get_client",
        "auth0_create_client",
        "auth0_update_client"
      ]
    },
    "git": {
      "command": "npx",
      "args": [
        "@cyanheads/git-mcp-server"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "mcp_git_git_status",
        "mcp_git_git_log",
        "mcp_git_git_diff_unstaged",
        "git_status",
        "git_set_working_dir",
        "git_add",
        "git_commit",
        "git_push"
      ]
    },
    "sqlite": {
      "command": "npx",
      "args": [
        "mcp-server-sqlite-npx",
        "./data/app.db"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "mcp_sqlite_read_query",
        "mcp_sqlite_list_tables",
        "mcp_sqlite_describe_table",
        "list_tables"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "add_memory",
        "search_memory",
        "get_memory",
        "list_memories",
        "create_entities",
        "search_nodes"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "@upstash/context7-mcp"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "search_docs",
        "get_documentation",
        "list_libraries",
        "resolve-library-id"
      ]
    }
  }
}