{
  "name": "cloudtolocalllm-tunnel-container",
  "version": "1.0.0",
  "description": "Simplified tunnel-aware container for CloudToLocalLLM using standard HTTP libraries",
  "main": "proxy-server.js",
  "type": "module",
  "scripts": {
    "start": "node proxy-server.js",
    "health": "node health-check.js",
    "test-tunnel": "node test-tunnel.js",
    "dev": "node --inspect proxy-server.js",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "tunnel",
    "container",
    "http-client",
    "multi-tenant",
    "cloudtolocalllm"
  ],
  "author": "CloudToLocalLLM",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@sentry/node": "^10.5.0",
    "@sentry/profiling-node": "^10.5.0",
    "express": "^5.1.0",
    "prom-client": "^15.1.3",
    "winston": "^3.11.0",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.7.2"
  }
}
