{
  "name": "@cloudtolocalllm/sdk",
  "version": "2.0.0",
  "description": "Official JavaScript/TypeScript SDK for CloudToLocalLLM API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cloudtolocalllm",
    "sdk",
    "api",
    "typescript",
    "javascript"
  ],
  "author": "CloudToLocalLLM Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudToLocalLLM/cloudtolocalllm.git"
  },
  "bugs": {
    "url": "https://github.com/CloudToLocalLLM/cloudtolocalllm/issues"
  },
  "homepage": "https://cloudtolocalllm.online"
}
